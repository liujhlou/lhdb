5" height="30"/><row row-number="6" height="27"/><row row-number="7" height="27"/><row row-number="8" height="27"/><row row-number="9" height="27"/><column col-number="1" width="63"/><column col-number="2" width="149"/><column col-number="3" width="120"/><column col-number="4" width="85"/><datasource name="tbs" type="jdbc" username="root" password="Database1!" url="jdbc:mysql://localhost:3306/tbs?useUnicode=true&amp;characterEncoding=UTF-8" driver="com.mysql.jdbc.Driver"><dataset name="集体签批表" type="sql"><sql><![CDATA[SELECT 
	a.PROJ_NAME,a.memo,a.A_ROLE_ID,a.B_ROLE_ID,b.sn,b.MEETINGTYPE,b.KEYIN_ID,c.USERNAME_,d.DEPT_ID_,e.CNAME,e.DEPTNAME,if(substring(f.opinion,6)='','',substring(f.opinion,6)) '审批意见'
FROM tbs.tbs_proj a 
left join 
    tbs.tbs_projcfm2 b on a.id = b.proj_id 
left join
	tbs.bdf2_user c on c.id = a.A_ROLE_ID
left join 
	tbs.bdf2_user_dept d on d.USERNAME_ = c.USERNAME_ 
left join 
	tbs.tbs_approver e on e.DEPTID = d.DEPT_ID_ and e.TITLE like '%经理%' and e.TITLE not like '%经理助理%'
left join 
	tbs.tbs_fun_apprc f on f.ROOTPID = b.BY3 and f.NODENAME = '评审会秘书录入会议决议单' and f.opinion is not NULL
where 
	a.id = :id  ;]]></sql><field name="PROJ_NAME"/><field name="memo"/><field name="A_ROLE_ID"/><field name="B_ROLE_ID"/><field name="sn"/><field name="MEETINGTYPE"/><field name="KEYIN_ID"/><field name="USERNAME_"/><field name="DEPT_ID_"/><field name="CNAME"/><field name="DEPTNAME"/><field name="审批意见"/><parameter name="id" type="String" default-value="279"/></dataset><dataset name="员工" type="sql"><sql><![CDATA[select * from bdf2_user]]></sql><field name="ID"/><field name="USERNAME_"/><field name="ADDRESS_"/><field name="ADMINISTRATOR_"/><field name="BIRTHDAY_"/><field name="CNAME_"/><field name="COMPANY_ID_"/><field name="CREATE_DATE_"/><field name="EMAIL_"/><field name="ENABLED_"/><field name="ENAME_"/><field name="MALE_"/><field name="MOBILE_"/><field name="PASSWORD_"/><field name="SALT_"/></dataset><dataset name="审批意见" type="sql"><sql><![CDATA[SELECT a.uid,a.OUTCOME,c.OPINION, c.ASSIGNEE,c.ENDDATE,c.ROOTPID  FROM tbs.tbs_proj_opinion  a
left join 
	tbs.bdf2_user u on u.ID = a.UID
left join 
	tbs.tbs_projcfm2 b on b.ID = a.CFM2_ID 
left join
	tbs.tbs_fun_apprc c on c.ROOTPID =b.BY3 and c.TASKNAME = '评委审批' and u.USERNAME_ = c.ASSIGNEE
where a.proj_id = :id and a.CFMTYPE=2 ;]]></sql><field name="uid"/><field name="OUTCOME"/><field name="OPINION"/><field name="ASSIGNEE"/><field name="ENDDATE"/><field name="ROOTPID"/><parameter name="id" type="String" default-value="279"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
