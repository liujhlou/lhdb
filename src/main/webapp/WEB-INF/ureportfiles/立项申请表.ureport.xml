column col-number="2" width="173"/><column col-number="3" width="48"/><column col-number="4" width="143"/><datasource name="tbs" type="jdbc" username="root" password="Database1!" url="jdbc:mysql://localhost:3306/tbs" driver="com.mysql.jdbc.Driver"><dataset name="立项审批表" type="sql"><sql><![CDATA[SELECT 
	a.PROJ_NAME,h.deptname,h.CNAME,a.a_role_id,a.b_role_id,a.memo MEMOA,b.memo MEMOB,c.taskname ,c.OPINION,c.ENDDATE,c.ASSIGNEE,c.STATE,concat('编号：',a.sn) SN
FROM tbs.tbs_proj a 
left join 
    tbs.tbs_projeaa b on a.id = b.proj_id 
left join 
	tbs.tbs_fun_apprc c on a.by3 = c.rootpid 
left join
	tbs.bdf2_user f on f.id = a.A_ROLE_ID
left join 
	tbs.bdf2_user_dept g on g.USERNAME_ = f.USERNAME_ 
left join 
	tbs.tbs_approver h on h.DEPTID = g.DEPT_ID_ and h.TITLE like '%经理%' and h.TITLE not like '%经理助理%'
where a.id = :id and taskname <> '开始' order by ENDDATE;]]></sql><field name="PROJ_NAME"/><field name="deptname"/><field name="CNAME"/><field name="a_role_id"/><field name="b_role_id"/><field name="MEMOA"/><field name="MEMOB"/><field name="taskname"/><field name="OPINION"/><field name="ENDDATE"/><field name="ASSIGNEE"/><field name="STATE"/><field name="SN"/><parameter name="id" type="String" default-value="257"/></dataset><dataset name="部门" type="sql"><sql><![CDATA[select * from bdf2_dept]]></sql><field name="DID"/><field name="ID_"/><field name="COMPANY_ID_"/><field name="CREATE_DATE_"/><field name="DESC_"/><field name="NAME_"/><field name="PARENT_ID_"/><field name="DEL"/></dataset><dataset name="员工" type="sql"><sql><![CDATA[select * from bdf2_user]]></sql><field name="ID"/><field name="USERNAME_"/><field name="ADDRESS_"/><field name="ADMINISTRATOR_"/><field name="BIRTHDAY_"/><field name="CNAME_"/><field name="COMPANY_ID_"/><field name="CREATE_DATE_"/><field name="EMAIL_"/><field name="ENABLED_"/><field name="ENAME_"/><field name="MALE_"/><field name="MOBILE_"/><field name="PASSWORD_"/><field name="SALT_"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
