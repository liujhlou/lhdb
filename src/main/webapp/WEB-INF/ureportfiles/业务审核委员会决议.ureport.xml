	a.PROJ_NAME,a.memo,a.a_role_id,a.b_role_id,b.by2,date_format(b.MEETINGDATE,"%y年%m月%d日") MEETINGDATE,b.MEETINGLOC,b.MEETINGTYPE,b.VTOTLOC/10000 VTOTLOC,b.VfaLOC/10000 vfaLOC,b.vnfaLOC/10000 vnfaLOC,b.PERIOD_CFM,b.GATRATE,b.GATRATEU,b.PSFY,b.deptdid,b.MGMUID,b.attend,c.TASKNAME,c.OPINION,d.AFLB_ID,b.TOBE,b.ACTBE,b.NOBE,b.AGREE,b.OPOOSE,b.DEBARB,b.WAIVER,e.name_ '客户名称',c.ENDDATE,c.ASSIGNEE '任务处理人',b.keyin_id '录入人'
FROM tbs.tbs_proj a 
left join 
    tbs.tbs_projcfm1 b on a.id = b.proj_id 
left join 
	tbs.tbs_fun_apprc c on b.by3 = c.rootpid 
left join
	tbs.tbs_proj_bank d on d.PROJ_ID = a.ID
left join
	tbs.tbs_customer e on a.CUS_ID = e.ID
where 
	a.id = :id and c.TASKNAME not in ('开始' , '风管经理审批' , '评审会秘书录入会议信息' , '评审会秘书确认' , '评审会秘书录入会议决议单') order by c.ENDDATE ;]]></sql><field name="PROJ_NAME"/><field name="memo"/><field name="a_role_id"/><field name="b_role_id"/><field name="by2"/><field name="MEETINGDATE"/><field name="MEETINGLOC"/><field name="MEETINGTYPE"/><field name="VTOTLOC"/><field name="vfaLOC"/><field name="vnfaLOC"/><field name="PERIOD_CFM"/><field name="GATRATE"/><field name="GATRATEU"/><field name="PSFY"/><field name="deptdid"/><field name="MGMUID"/><field name="attend"/><field name="TASKNAME"/><field name="OPINION"/><field name="AFLB_ID"/><field name="TOBE"/><field name="ACTBE"/><field name="NOBE"/><field name="AGREE"/><field name="OPOOSE"/><field name="DEBARB"/><field name="WAIVER"/><field name="客户名称"/><field name="ENDDATE"/><field name="任务处理人"/><field name="录入人"/><parameter name="id" type="String" default-value="256"/></dataset><dataset name="部门" type="sql"><sql><![CDATA[select * from bdf2_dept]]></sql><field name="DID"/><field name="ID_"/><field name="COMPANY_ID_"/><field name="CREATE_DATE_"/><field name="DESC_"/><field name="NAME_"/><field name="PARENT_ID_"/><field name="DEL"/></dataset><dataset name="员工" type="sql"><sql><![CDATA[select * from bdf2_user]]></sql><field name="ID"/><field name="USERNAME_"/><field name="ADDRESS_"/><field name="ADMINISTRATOR_"/><field name="BIRTHDAY_"/><field name="CNAME_"/><field name="COMPANY_ID_"/><field name="CREATE_DATE_"/><field name="EMAIL_"/><field name="ENABLED_"/><field name="ENAME_"/><field name="MALE_"/><field name="MOBILE_"/><field name="PASSWORD_"/><field name="SALT_"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
