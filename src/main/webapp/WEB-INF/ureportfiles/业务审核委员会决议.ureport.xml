er="9" height="35"/><row row-number="10" height="35"/><row row-number="11" height="36"/><column col-number="1" width="68"/><column col-number="2" width="191"/><column col-number="3" width="93"/><column col-number="4" width="65"/><datasource name="tbs" type="jdbc" username="root" password="Database1!" url="jdbc:mysql://localhost:3306/tbs?useUnicode=true&amp;characterEncoding=UTF-8" driver="com.mysql.jdbc.Driver"><dataset name="业务审核委员会决议" type="sql"><sql><![CDATA[select * 
from(
SELECT 
	concat( (select  name_ from tbs_customer   where id=a.cus_id), (select round((b.VTOTLOC/10000),2)),'万元综合授信项目') as PROJ_NAME,b.id,a.a_role_id,a.b_role_id,b.by2,b.MEETINGDATE MEETINGDATE,b.MEETINGLOC,b.MEETINGTYPE,h.deptname,h.CNAME,b.attend,c.TASKNAME,b.decision 审批意见,c.decs,c.OPINION,b.TOBE,b.ACTBE,b.NOBE,b.AGREE,b.OPOOSE,b.DEBARB,b.WAIVER,e.name_ '客户名称',c.ENDDATE,c.ASSIGNEE '任务处理人',b.keyin_id '录入人'
FROM tbs.tbs_proj a 
left join 
    tbs.tbs_projcfm1 b on a.id = b.proj_id 
left join 
	tbs.tbs_fun_apprc c on b.by3 = c.rootpid and c.TASKNAME not in ('开始' , '风管经理审批' , '评审会秘书录入会议信息' , '评审会秘书确认' ,'三要素变更-风管经理审批','三要素变更-评审会秘书录入会议信息','三要素变更-评审会秘书确认') and c.opinion is not NULL
left join
	tbs.tbs_customer e on a.CUS_ID = e.ID

left join
	tbs.bdf2_user z on z.id = a.A_ROLE_ID
left join 
	tbs.bdf2_user_dept g on g.USERNAME_ = z.USERNAME_ 
left join 
	tbs.tbs_approver h on h.DEPTID = g.DEPT_ID_ and h.TITLE like '%经理%' and h.TITLE not like '%经理助理%'

where 
	a.id = :id and b.id = :cfmid group by nodename) temp  order by ENDDATE ;]]></sql><field name="PROJ_NAME"/><field name="id"/><field name="a_role_id"/><field name="b_role_id"/><field name="by2"/><field name="MEETINGDATE"/><field name="MEETINGLOC"/><field name="MEETINGTYPE"/><field name="deptname"/><field name="CNAME"/><field name="attend"/><field name="TASKNAME"/><field name="审批意见"/><field name="decs"/><field name="OPINION"/><field name="TOBE"/><field name="ACTBE"/><field name="NOBE"/><field name="AGREE"/><field name="OPOOSE"/><field name="DEBARB"/><field name="WAIVER"/><field name="客户名称"/><field name="ENDDATE"/><field name="任务处理人"/><field name="录入人"/><parameter name="id" type="String" default-value="323"/><parameter name="cfmid" type="String" default-value="0"/></dataset><dataset name="部门" type="sql"><sql><![CDATA[select * from bdf2_dept]]></sql><field name="DID"/><field name="ID_"/><field name="COMPANY_ID_"/><field name="CREATE_DATE_"/><field name="DESC_"/><field name="NAME_"/><field name="PARENT_ID_"/><field name="DEL"/></dataset><dataset name="员工" type="sql"><sql><![CDATA[select * from bdf2_user]]></sql><field name="ID"/><field name="USERNAME_"/><field name="ADDRESS_"/><field name="ADMINISTRATOR_"/><field name="BIRTHDAY_"/><field name="CNAME_"/><field name="COMPANY_ID_"/><field name="CREATE_DATE_"/><field name="EMAIL_"/><field name="ENABLED_"/><field name="ENAME_"/><field name="MALE_"/><field name="MOBILE_"/><field name="PASSWORD_"/><field name="SALT_"/></dataset><dataset name="审批结果" type="sql"><sql><![CDATA[SELECT a.uid,a.OUTCOME,a.COMMENT, c.ASSIGNEE,a.TIMESTAMP_UPDATE,c.ROOTPID  FROM tbs.tbs_proj_opinion  a
left join 
	tbs.bdf2_user u on u.ID = a.UID
left join 
	tbs.tbs_projcfm1 b on b.ID = a.CFM1_ID 
left join
	tbs.tbs_fun_apprc c on c.ROOTPID =b.BY3 and c.TASKNAME = '评委审批' and u.USERNAME_ = c.ASSIGNEE 
where a.proj_id = :id and a.CFMTYPE=1 and a.del=0 and b.id = :cfmid;]]></sql><field name="uid"/><field name="OUTCOME"/><field name="COMMENT"/><field name="ASSIGNEE"/><field name="TIMESTAMP_UPDATE"/><field name="ROOTPID"/><parameter name="id" type="String" default-value="323"/><parameter name="cfmid" type="String" default-value="0"/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
