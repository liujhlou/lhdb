	b.SN,c.ZHT,c.BZHT,d.NAME_,a.RLSDATE ,e.BIZTYPE,e.BIZVAR,d.ENTPROPERTY_ID,d.ENTSCALE_ID,d.INDUTYPE_ID,
    f.AFLB_ID,f.AFLsB_ID,b.APPTOTLOC,a.TOTLOC,c.UNDBDATE,c.UNDEDATE,c.BY3,a.memo '解保备注'
FROM tbs.tbs_proj_release a 
left join tbs.tbs_projundwrt b on a.UNDWRT_ID = b.ID and b.DEL = 0 and b.valid = 1
left join tbs.tbs_projundwrt_cfmar c on a.UNDWRT_ID = c.PROJUNDWRT_ID and c.del = 0
left join tbs.tbs_customer d on b.CUS_ID = d.ID
left join (SELECT me.*,GROUP_CONCAT(aa.NAME_) BIZTYPE,GROUP_CONCAT(bb.NAME_) BIZVAR FROM tbs.tbs_projundwrt_bizvt me 
			left join tbs.tbs_bas_biztype aa  on  aa.id = me.BIZTYPE_ID 
			left join tbs.tbs_bas_bizvar bb on bb.id = me.BIZVAR_ID 
			where me.del = 0
			group by me.PROJUNDWRT_ID) e on a.UNDWRT_ID = e.PROJUNDWRT_ID and e.del = 0
