"25"/><row row-number="10" height="25"/><row row-number="11" height="25"/><row row-number="12" height="25"/><row row-number="13" height="25"/><column col-number="1" width="71"/><column col-number="2" width="113"/><column col-number="3" width="80"/><column col-number="4" width="154"/><datasource name="tbs" type="jdbc" username="root" password="Database1!" url="jdbc:mysql://localhost:3306/tbs?useUnicode=true&amp;characterEncoding=UTF-8" driver="com.mysql.jdbc.Driver"><dataset name="承保审批表" type="sql"><sql><![CDATA[SELECT 
	concat('编号：',b.SN) '编号', b.bdate,b.edate, b.undbdate,b.undedate,
    a.PROJ_NAME,a.memo,a.INITTOTLOC '授信总额',b.APPTOTLOC '本次申请金额',b.RLSTOTLOC  '可用解保余额',b.by3 '期限',b.GATRATE,b.GATRATEU,b.PSFY,b.FINANCIAL,b.ZXFY,b.GATRECKON,c.TASKNAME,ifnull(c.OPINION,'通过-- 审批意见：同意') as '审批意见',
    d.AFLB_ID '分行',d.AFLSB_ID '支行',e.BIZTYPE_ID '业务类型',e.BIZVAR_ID '业务品种',c.ASSIGNEE ,c.ENDDATE,a.by5 '项目简介'
FROM tbs.tbs_projundwrt b 
left join 
    tbs.tbs_proj a on a.id = b.proj_id 
left join 
	tbs.tbs_fun_apprc c on b.PID = c.rootpid and c.taskname <> '驳回修正'
left join 
	tbs.tbs_projundwrt_bank d on d.PROJUNDWRT_ID = b.ID
left join 
	tbs.tbs_projundwrt_bizvt e on e.PROJUNDWRT_ID = b.ID and e.del=0
inner join 
