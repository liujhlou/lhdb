            <Buttons>
              <Button>
                <ClientEvent name="onClick">var current = view.id(&quot;dataSetCalendar&quot;).getData(&quot;#.#calendarDates&quot;);&#xD;
var currentDef = view.id(&quot;dataSetCalendar&quot;).getData(&quot;#&quot;);&#xD;
if(currentDef.get(&quot;type&quot;)==&quot;daily&quot;){&#xD;
	current.set(&quot;rangeStartTime&quot;,view.id(&quot;dateTimeSpinnerStart&quot;).get(&quot;text&quot;));&#xD;
	current.set(&quot;rangeEndTime&quot;,view.id(&quot;dateTimeSpinnerEnd&quot;).get(&quot;text&quot;));&#xD;
	var startTime = current.get(&quot;rangeStartTime&quot;);&#xD;
	var endTime = current.get(&quot;rangeEndTime&quot;);&#xD;
	var startHour = parseInt(startTime.substring(0,2)),startMinute = parseInt(startTime.substring(3,5)),startSecond = parseInt(startTime.substring(6,8));&#xD;
	var endHour = parseInt(endTime.substring(0,2)),endMinute = parseInt(endTime.substring(3,5)),endSecond = parseInt(endTime.substring(6,8));&#xD;
	var isValid = false;&#xD;
	if(startHour &lt; endHour){&#xD;
		isValid = true;&#xD;
	}else if(startHour == endHour){&#xD;
		if(startMinute &lt; endMinute){&#xD;
			isValid = true;&#xD;
		}else if(startMinute == endMinute){&#xD;
			if(startSecond&lt;=endSecond){&#xD;
				isValid = true;&#xD;
			}&#xD;
		}&#xD;
	}&#xD;
	if(!isValid){&#xD;
		dorado.MessageBox.alert(&quot;开始时间不能大于结束时间！&quot;);&#xD;
		return;&#xD;
	}&#xD;
}&#xD;
view.id(&quot;updateActionSaveCalendar&quot;).execute(function(){&#xD;
	view.id(&quot;dialogCalendarDate&quot;).hide();&#xD;
});&#xD;
</ClientEvent>
