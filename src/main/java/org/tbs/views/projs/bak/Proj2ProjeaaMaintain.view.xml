        <Property name="label">录入人</Property>
        <Property name="readOnly">true</Property>
      </PropertyDef>
      <PropertyDef name="VOcname">
        <Property name="label">录入人</Property>
        <Property name="readOnly">true</Property>
        <Property name="defaultValue">${loginUser.getCname()}</Property>
      </PropertyDef>
      <PropertyDef name="tbsBasPs">
        <Property name="label">项目状态</Property>
        <Property name="dataType">datatypeTbsBasPs</Property>
        <Property name="readOnly">true</Property>
        <Property name="defaultValue">1</Property>
      </PropertyDef>
      <PropertyDef name="valid">
        <Property name="label">是否通过审批</Property>
      </PropertyDef>
      <Reference name="tbsProjBizvtSet">
        <Property name="dataType">[datatypeTbsProjBizvt]</Property>
        <Property name="dataProvider">proj2ProjeaaDao#loadTbsProjBizvt</Property>
        <Property name="parameter">$${this.id}</Property>
      </Reference>
      <Reference name="tbsProjCggSet">
        <Property name="dataType">[datatypeTbsProjCgg]</Property>
        <Property name="dataProvider">proj2ProjeaaDao#loadTbsProjCgg</Property>
        <Property name="parameter">$${this.id}</Property>
      </Reference>
      <Reference name="tbsProjBankSet">
        <Property name="parameter">$${this.id}</Property>
        <Property name="dataType">[datatypeTbsProjBank]</Property>
        <Property name="dataProvider">proj2ProjeaaDao#loadTbsProjBank</Property>
      </Reference>
    </DataType>
    <DataType name="datatypeTbsCustomer" parent="global:dataTypeTbsCustomer">
      <Property name="defaultDisplayProperty">name</Property>
    </DataType>
    <DataType name="datatypeTbsBasConsway" parent="global:dataTypeTbsBasConsway">
      <Property name="defaultDisplayProperty">name</Property>
    </DataType>
    <DataType name="datatypeTbsBasPs" parent="global:dataTypeTbsBasPs">
      <Property name="defaultDisplayProperty">name</Property>
    </DataType>
    <DataType name="datatypeTbsBasGovfund" parent="dataTypeTbsBasGovfund">
      <Property name="defaultDisplayProperty">name</Property>
    </DataType>
    <DataType name="datatypeTbsBasCurrency" parent="dataTypeTbsBasCurrency">
      <Property name="defaultDisplayProperty">name</Property>
    </DataType>
    <DataType name="datatypeBdf2User" parent="dataTypeBdf2User">
      <Property name="defaultDisplayProperty">cname</Property>
    </DataType>
    <DataType name="datatypeTbsProjBizvt" parent="dataTypeTbsProjBizvt">
      <PropertyDef name="tbsBasBiztype">
        <Property name="label">业务类型</Property>
        <Property name="dataType">datatypeTbsBasBiztype</Property>
      </PropertyDef>
      <PropertyDef name="tbsBasBizvar">
        <Property name="dataType">datatypeTbsBasBizvar</Property>
        <Property name="label">业务品种</Property>
      </PropertyDef>
      <PropertyDef name="loc">
        <Property name="dataType">BigDecimal</Property>
        <Property name="label">申请授信金额</Property>
      </PropertyDef>
      <PropertyDef name="vloc">
        <Property name="dataType">BigDecimal</Property>
        <Property name="label">可用授信金额</Property>
      </PropertyDef>
      <PropertyDef name="valid">
        <Property name="dataType">boolean</Property>
        <Property name="label">是否通过审批</Property>
      </PropertyDef>
    </DataType>
    <DataType name="datatypeTbsProjCgg" parent="dataTypeTbsProjCgg"/>
    <DataType name="datatypeTbsProjBank" parent="dataTypeTbsProjBank">
      <Property name="defaultDisplayProperty">name</Property>
      <PropertyDef name="tbsBasBank_M">
        <Property name="dataType">datatypeTbsBasBank</Property>
        <Property name="label">申请贷款银行(支行)</Property>
      </PropertyDef>
      <PropertyDef name="tbsBasBank_S">
        <Property name="label">申请贷款支行(分行)</Property>
        <Property name="dataType">datatypeTbsBasBank</Property>
      </PropertyDef>
    </DataType>
    <DataType name="datatypeTbsBasBizvar" parent="dataTypeTbsBasBizvar">
      <Property name="defaultDisplayProperty">name</Property>
    </DataType>
    <DataType name="datatypeTbsBasBiztype" parent="dataTypeTbsBasBiztype">
      <Property name="defaultDisplayProperty">name</Property>
      <Reference name="tbsBasBizvarSet">
        <Property name="dataProvider">tbsBasBiztypevarDao#loadVar</Property>
        <Property name="cacheMode">bothSides</Property>
        <Property name="pageSize">15</Property>
        <Property name="dataType">[datatypeTbsBasBizvar]</Property>
        <Property name="parameter">$${this.id}</Property>
      </Reference>
    </DataType>
    <DataType name="datatypeTbsBasBank" parent="dataTypeTbsBasBank">
      <Property name="defaultDisplayProperty">name</Property>
      <Reference name="tbsBasBankSet">
        <Property name="dataType">[SELF]</Property>
        <Property name="dataProvider">tbsBasBankDao#loadSub</Property>
        <Property name="cacheMode">bothSides</Property>
        <Property name="parameter">$${this.id}</Property>
      </Reference>
    </DataType>
    <DataType name="tempConditions">
      <PropertyDef name="valid">
        <Property name="defaultValue">2</Property>
        <Property name="dataType">Integer</Property>
      </PropertyDef>
      <PropertyDef name="acptdate">
        <Property name="displayFormat">Y-m-d</Property>
        <Property name="dataType">Date</Property>
      </PropertyDef>
      <PropertyDef name="projName"/>
    </DataType>
  </Model>
  <View>
    <Property name="packages">silk,font-awesome,entypo</Property>
    <DataSet id="datasetTbsProj">
      <Property name="dataProvider">proj2ProjeaaDao#getAllToPage</Property>
      <Property name="dataType">[datatypeTbsProj]</Property>
      <Property name="loadMode">lazy</Property>
      <Property name="pageSize">15</Property>
    </DataSet>
    <Container id="ddlDataSet">
      <DataSet id="datasetTbsCustomer">
        <Property name="dataType">[datatypeTbsCustomer]</Property>
        <Property name="dataProvider">tbsCustomerDao#getAllByName</Property>
      </DataSet>
      <DataSet id="datasetTbsBasConsway">
        <Property name="dataType">[datatypeTbsBasConsway]</Property>
        <Property name="dataProvider">tbsBasConswayDao#loadAll</Property>
      </DataSet>
      <DataSet id="datasetTbsBasCurrency">
        <Property name="dataType">[datatypeTbsBasCurrency]</Property>
        <Property name="dataProvider">tbsBasCurrencyDao#loadAll</Property>
      </DataSet>
      <DataSet id="datasetTbsBasPs">
        <Property name="dataType">[datatypeTbsBasPs]</Property>
        <Property name="dataProvider">tbsBasPsDao#getAll</Property>
      </DataSet>
      <DataSet id="datasetTbsBasGovfund">
        <Property name="dataType">[datatypeTbsBasGovfund]</Property>
        <Property name="dataProvider">tbsBasGovfundDao#loadAll</Property>
      </DataSet>
      <DataSet id="datasetBdf2UserA">
        <Property name="dataProvider">bdf2UserDao#getAllByName</Property>
        <Property name="dataType">[datatypeBdf2User]</Property>
      </DataSet>
      <DataSet id="datasetBdf2UserB">
        <Property name="dataProvider">bdf2UserDao#getAllByName</Property>
        <Property name="dataType">[datatypeBdf2User]</Property>
      </DataSet>
      <DataSet id="datasetTbsBasBiztype">
        <Property name="dataProvider">tbsBasBiztypevarDao#loadBiztype</Property>
        <Property name="dataType">[datatypeTbsBasBiztype]</Property>
      </DataSet>
      <DataSet id="datasetTbsBasBizvar">
        <Property name="dataProvider">tbsBasBizvarDao#getAll</Property>
        <Property name="dataType">[datatypeTbsBasBizvar]</Property>
        <Property name="cacheable">true</Property>
      </DataSet>
      <DataSet id="datasetdatasetTbsBasBank">
        <Property name="dataProvider">tbsBasBankDao#loadMain</Property>
        <Property name="dataType">[datatypeTbsBasBank]</Property>
      </DataSet>
    </Container>
    <AutoForm id="autoformCondition">
      <Property name="createPrivateDataSet">true</Property>
      <Property name="cols">*,*,*</Property>
      <Property name="dataType">tempConditions</Property>
      <Property name="width">100%</Property>
      <AutoFormElement>
        <Property name="name">projName</Property>
        <Property name="property">projName</Property>
        <Property name="label">项目名称</Property>
        <Property name="width">100%</Property>
        <Editor/>
      </AutoFormElement>
      <AutoFormElement>
        <Property name="name">tbsCustomer</Property>
        <Property name="property">tbsCustomer</Property>
        <Property name="label">客户名称</Property>
        <Property name="trigger">ddlTbsCustomer</Property>
        <Property name="width">100%</Property>
        <Editor/>
      </AutoFormElement>
      <AutoFormElement>
        <Property name="name">tbsBasConsway</Property>
        <Property name="property">tbsBasConsway</Property>
        <Property name="label">咨询方式</Property>
        <Property name="trigger">ddlTbsBasConsway</Property>
        <Property name="width">100%</Property>
        <Editor/>
      </AutoFormElement>
      <AutoFormElement>
        <Property name="name">acptdate</Property>
        <Property name="property">acptdate</Property>
        <Property name="label">受理日期</Property>
        <Property name="editable">true</Property>
        <Property name="trigger">defaultDateDropDown</Property>
        <Property name="width">100%</Property>
        <Editor/>
      </AutoFormElement>
    </AutoForm>
    <ToolBar>
      <ToolBarButton id="buttonQuery">
        <Property name="caption">查 询</Property>
        <Property name="iconClass">silk silk-zoom</Property>
      </ToolBarButton>
      <ToolBarButton id="buttonEdit">
        <Property name="caption">明 细</Property>
        <Property name="iconClass">silk silk-application-view-detail</Property>
      </ToolBarButton>
      <ToolBarButton id="buttonCreation">
        <Property name="caption">生成立项审批单</Property>
        <Property name="iconClass">silk silk-star</Property>
      </ToolBarButton>
      <DataPilot>
        <Property name="itemCodes">pages</Property>
        <Property name="dataSet">datasetTbsProj</Property>
      </DataPilot>
    </ToolBar>
    <Container id="UI">
      <Property name="height">82%</Property>
      <SplitPanel>
        <Property name="direction">top</Property>
        <Property name="position">60%</Property>
        <MainControl>
          <TabControl>
            <ControlTab>
              <Property name="caption">银行信息</Property>
              <Container id="UIDialogTbsProjBank">
                <DataGrid id="UIdatagridTbsProjBank">
                  <Property name="dataSet">datasetTbsProj</Property>
                  <Property name="dataPath">#.tbsProjBankSet</Property>
                  <Property name="width">600</Property>
                  <Property name="readOnly">true</Property>
                  <RowNumColumn/>
                  <DataColumn name="tbsBasBank_M">
                    <Property name="property">tbsBasBank_M</Property>
                    <Property name="trigger">ddlTbsBasBank_M</Property>
                  </DataColumn>
                  <DataColumn name="tbsBasBank_S">
                    <Property name="property">tbsBasBank_S</Property>
                    <Property name="trigger">ddlTbsBasBank_S</Property>
                  </DataColumn>
                </DataGrid>
              </Container>
            </ControlTab>
            <ControlTab>
              <Property name="caption">业务类品金</Property>
              <Container id="UIDialogTbsProjBizvt">
