                  <Property name="readOnly">true</Property>
                  <RowNumColumn/>
                  <DataColumn name="tbsBasBiztype">
                    <Property name="property">tbsBasBiztype</Property>
                    <Property name="trigger">ddlTbsBasBiztype</Property>
                  </DataColumn>
                  <DataColumn name="tbsBasBizvar">
                    <Property name="property">tbsBasBizvar</Property>
                    <Property name="trigger">ddlTbsBasBizvar</Property>
                  </DataColumn>
                  <DataColumn name="loc">
                    <Property name="property">loc</Property>
                  </DataColumn>
                  <DataColumn name="vloc">
                    <Property name="property">vloc</Property>
                  </DataColumn>
                </DataGrid>
              </Container>
            </ControlTab>
            <ControlTab>
              <Property name="caption">反担保信息</Property>
              <Container id="UIDialogTbsProjCgg">
                <DataGrid id="UIdatagridTbsProjCgg">
                  <Property name="dataSet">datasetTbsProj</Property>
                  <Property name="dataPath">#.tbsProjCggSet</Property>
