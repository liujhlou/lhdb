      </PropertyDef>
      <PropertyDef name="bdf2User_A">
        <Property name="dataType">datatypeBdf2User</Property>
        <Property name="label">项目经理A</Property>
      </PropertyDef>
      <PropertyDef name="bdf2User_B">
        <Property name="dataType">datatypeBdf2User</Property>
        <Property name="label">项目经理B</Property>
      </PropertyDef>
